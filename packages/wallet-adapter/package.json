  "scripts": {
    "build": "tsup-node",
    "dev": "tsup-node --watch"
  },
  "dependencies": {
    "events": "^3.3.0",
    "neverthrow": "^5.0.0",
    "xstate": "^5.17.4"
  },
  "devDependencies": {
    "@types/events": "^3.0.3",
    "tsup": "^8.2.0",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "@dfinity/agent": "^1.3",
    "@dfinity/auth-client": "^1.3",
    "@dfinity/candid": "^1.3",
    "@dfinity/identity": "^1.3",
    "@dfinity/principal": "^1.3"
  }
