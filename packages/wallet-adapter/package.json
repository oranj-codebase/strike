{
  "name": "@oranjlabs/icp-wallet-adapter",
  "author": "Edward Lee <edward@oranj.co>",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
